# Vibber Environment Configuration
# Copy this file to .env and fill in the values

# =============================================================================
# GENERAL
# =============================================================================
ENV=development
# ENV=production

# =============================================================================
# DATABASE
# =============================================================================
DATABASE_URL=postgres://postgres:postgres@localhost:5432/vibber?sslmode=disable
REDIS_URL=redis://localhost:6379

# =============================================================================
# MESSAGE QUEUE
# =============================================================================
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# =============================================================================
# SECURITY
# =============================================================================
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# =============================================================================
# AI SERVICES
# =============================================================================
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api...

# Get your API key from https://platform.openai.com/
OPENAI_API_KEY=sk-...

# Get your API key from https://www.pinecone.io/
PINECONE_API_KEY=
PINECONE_INDEX=vibber-agents
PINECONE_ENVIRONMENT=us-east-1

# =============================================================================
# OAUTH PROVIDERS
# =============================================================================
# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# GitHub OAuth
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# =============================================================================
# INTEGRATION CREDENTIALS
# =============================================================================
# Slack App
SLACK_CLIENT_ID=
SLACK_CLIENT_SECRET=
SLACK_SIGNING_SECRET=

# Jira/Confluence (Atlassian)
JIRA_CLIENT_ID=
JIRA_CLIENT_SECRET=

# =============================================================================
# ANALYTICS
# =============================================================================
# Get your token from https://mixpanel.com/
REACT_APP_MIXPANEL_TOKEN=

# =============================================================================
# FRONTEND
# =============================================================================
REACT_APP_API_URL=http://localhost:8080/api/v1
FRONTEND_URL=http://localhost:3000

# =============================================================================
# SERVICE URLS (Internal)
# =============================================================================
AGENT_SERVICE_URL=http://localhost:8000
